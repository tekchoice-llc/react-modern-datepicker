{
  "name": "react-modern-datepicker",
  "version": "1.2.0",
  "description": "A modern date picker for react library",
  "homepage": "https://github.com/talentedandrew/react-modern-datepicker#readme",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest",
    "build-old": "babel lib -d build",
    "clean": "rm -r dist && mkdir dist",
    "prebuild": "npm run clean",
    "build": "webpack",
    "build:watch": "babel lib -w -d build",
    "lint": "eslint lib/**; exit 0",
    "lint:watch": "esw -w lib/**",
    "lint:fix": "eslint lib/** --fix",
    "prepublish": "npm run test",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/talentedandrew/react-modern-datepicker.git"
  },
  "keywords": [
    "datepicker",
    "react-datepicker"
  ],
  "author": "Anurag Veekancheri",
  "license": "Apache License",
  "bugs": {
    "url": "https://github.com/talentedandrew/react-modern-datepicker/issues"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.13.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-watch": "^7.0.0",
    "jest": "^27.0.6",
    "polished": "^1.9.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "webpack": "^5.50.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^4.8.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "cleave.js": "^1.6.0",
    "dayjs": "^1.8.14",
    "styled-components": "^4.2.0"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  }
}
